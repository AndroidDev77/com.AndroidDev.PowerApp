<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="spivey.app.practice0001"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="10" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name=".PowerAppActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddHouseActivity"
            android:label="@string/title_activity_add_house" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="spivey.app.practice0001.PowerAppActivity" />
        </activity>
        <activity
            android:name=".HouseConfigActivity"
            android:label="@string/title_activity_house_config" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="spivey.app.practice0001.PowerAppActivity" />
        </activity>
        <activity
            android:name=".AmpDisplayActivity"
            android:label="@string/title_activity_amp_display" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="spivey.app.practice0001.PowerAppActivity" />
        </activity>
    </application>

</manifest>